\frametitle{Running \textsc{TIPSI}}

\begin{lstlisting}
maxframes  = 2000

par dh1  = dihdeg(frame$Dihedral1)
par dh2  = dihdeg(frame$Dihedral2)

state A = (-150 < dh1 & dh1 < -50
	& 120 < dh2 & dh2 < 180)
state B = (-100 < dh1 & dh1 < -50
	& -50 < dh2 & dh2 < 20)

interface I = (!A) & (!B)
\end{lstlisting}
